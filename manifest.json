{
    "name": "Clickme",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": " Best Clipboard Extensions for Google Chrome in 2022   ",
    "background": {
        "scripts": [
            "popup.js",
            "index.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js; object-src 'self'",
    "browser_action": {
        "default_title": "Clipme",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}