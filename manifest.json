{
    "name": "Darkme",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": " Best Dark Mode Extensions for Google Chrome in 2022   ",
    "background": {
        "scripts": [
            "popup.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Invert.io",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}